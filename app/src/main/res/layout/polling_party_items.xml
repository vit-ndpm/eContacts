<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="3dp"
    app:cardCornerRadius="10dp"
    app:cardElevation="4dp"
    app:cardUseCompatPadding="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:background="@drawable/nodal_border"
        android:paddingBottom="5dp"
        android:textAlignment="center">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Assembly:"
            app:layout_constraintBottom_toBottomOf="@+id/ac_name_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/ac_name_tv" />

        <TextView
            android:id="@+id/ac_name_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="ac name"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/booth_nameTv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/textView12"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView12"
            app:layout_constraintTop_toTopOf="@+id/textView12" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="Booth:"
            app:layout_constraintStart_toEndOf="@+id/ac_name_tv"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/po_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ac_name_tv">

            <TextView
                android:id="@+id/po_lable_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="P O :"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/po_name_tv"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/po_mobile" />

            <TextView
                android:id="@+id/po_mobile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:paddingHorizontal="10dp"
                android:text="9425966696"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/img_call_po"
                app:layout_constraintStart_toEndOf="@+id/po_lable_tv"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/img_call_po"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/po_mobile"
                app:layout_constraintEnd_toStartOf="@+id/img_sms_po"
                app:layout_constraintTop_toTopOf="@+id/po_mobile"
                app:srcCompat="@drawable/baseline_call_24" />

            <ImageView
                android:id="@+id/img_sms_po"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/img_call_po"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/img_call_po"
                app:srcCompat="@drawable/baseline_sms_24" />

            <TextView
                android:id="@+id/po_name_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:paddingHorizontal="10dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/po_mobile"
                app:layout_constraintStart_toStartOf="@+id/po_mobile"
                app:layout_constraintTop_toBottomOf="@+id/po_mobile" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/p1_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/po_layout">

            <TextView
                android:id="@+id/p1_lable_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="P 1 :"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/p1_name_tv"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/p1_mobile" />

            <TextView
                android:id="@+id/p1_mobile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:paddingHorizontal="10dp"
                android:text="9425966696"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/img_call_p1"
                app:layout_constraintStart_toEndOf="@+id/p1_lable_tv"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/img_call_p1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/p1_mobile"
                app:layout_constraintEnd_toStartOf="@+id/img_sms_p1"
                app:layout_constraintTop_toTopOf="@+id/p1_mobile"
                app:srcCompat="@drawable/baseline_call_24" />

            <ImageView
                android:id="@+id/img_sms_p1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/img_call_p1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/img_call_p1"
                app:srcCompat="@drawable/baseline_sms_24" />

            <TextView
                android:id="@+id/p1_name_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:paddingHorizontal="10dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/p1_mobile"
                app:layout_constraintStart_toStartOf="@+id/p1_mobile"
                app:layout_constraintTop_toBottomOf="@+id/p1_mobile" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/p2_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/p1_layout">

            <TextView
                android:id="@+id/p2_lable_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="P 2 :"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/p2_name_tv"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/p2_mobile" />

            <TextView
                android:id="@+id/p2_mobile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:paddingHorizontal="10dp"
                android:text="9425966696"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/img_call_p2"
                app:layout_constraintStart_toEndOf="@+id/p2_lable_tv"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/img_call_p2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/p2_mobile"
                app:layout_constraintEnd_toStartOf="@+id/img_sms_p2"
                app:layout_constraintTop_toTopOf="@+id/p2_mobile"
                app:srcCompat="@drawable/baseline_call_24" />

            <ImageView
                android:id="@+id/img_sms_p2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/img_call_p2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/img_call_p2"
                app:srcCompat="@drawable/baseline_sms_24" />

            <TextView
                android:id="@+id/p2_name_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:paddingHorizontal="10dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/p2_mobile"
                app:layout_constraintStart_toStartOf="@+id/p2_mobile"
                app:layout_constraintTop_toBottomOf="@+id/p2_mobile" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/p3_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/p2_layout">

            <TextView
                android:id="@+id/p3_lable_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="P 3 :"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/p3_name_tv"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/p3_mobile" />

            <TextView
                android:id="@+id/p3_mobile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:paddingHorizontal="10dp"
                android:text="9425966696"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/img_call_p3"
                app:layout_constraintStart_toEndOf="@+id/p3_lable_tv"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/img_call_p3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/p3_mobile"
                app:layout_constraintEnd_toStartOf="@+id/img_sms_p3"
                app:layout_constraintTop_toTopOf="@+id/p3_mobile"
                app:srcCompat="@drawable/baseline_call_24" />

            <ImageView
                android:id="@+id/img_sms_p3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/img_call_p3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/img_call_p3"
                app:srcCompat="@drawable/baseline_sms_24" />

            <TextView
                android:id="@+id/p3_name_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:paddingHorizontal="10dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/p3_mobile"
                app:layout_constraintStart_toStartOf="@+id/p3_mobile"
                app:layout_constraintTop_toBottomOf="@+id/p3_mobile" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>